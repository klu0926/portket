{
  "printWidth": 200,
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*.hbs",
      "options": {
        "parser": "glimmer",
        "singleQuote": false
      }
    },
    {
      "files": "*.js",
      "options": {
        "printWidth": 120
      }
    }
  ]
}
